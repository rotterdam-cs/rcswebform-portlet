<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE ivy-module [
    <!ENTITY jackson.version "2.6.0">
]>

<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
    <info organisation="com.rcs" module="template" status="integration">
    </info>
    <configurations>
		<conf name="default" visibility="public" description="runtime dependencies and master artifact can be used with this conf"/>		
	</configurations>
    <dependencies>
		<dependency org="com.fasterxml.jackson.core" name="jackson-core" rev="&jackson.version;" force="true" conf="default"/>
		<dependency org="com.fasterxml.jackson.core" name="jackson-annotations" rev="&jackson.version;" force="true" conf="default"/>
		<dependency org="com.fasterxml.jackson.core" name="jackson-databind" rev="&jackson.version;" force="true" conf="default"/>
	</dependencies>
</ivy-module>
