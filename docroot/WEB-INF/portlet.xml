<?xml version="1.0"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0">
	
	<portlet>
		<portlet-name>rcs-web-form</portlet-name>
		<display-name>RCS Web Form</display-name>
		<portlet-class>
			com.rcs.webform.portlet.WebFormPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/jsp/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
        <resource-bundle>content.Language</resource-bundle>
		<portlet-info>
			<title>RCS Web Form</title>
			<short-title>Rcs Web Form</short-title>
			<keywords></keywords>
		</portlet-info>
		<portlet-preferences>
			<preference>
				<name>description</name>
				<value>Here will be the description of RCS Web Form</value>
			</preference>
		</portlet-preferences>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>rcs-web-form-report</portlet-name>
		<display-name>RCS Web Form Report</display-name>
		<portlet-class>
			com.rcs.webform.portlet.WebFormReportPortlet
		</portlet-class>
		<init-param>
			<name>view-template</name>
			<value>/jsp/report/view.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<portlet-info>
			<title>RCS Web Form Report</title>
			<short-title>Rcs Web Form Report</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>