<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.rcs.dbService.model.WebformColumn" />
	<import class="com.rcs.dbService.model.WebformRow" />
	<import class="com.rcs.dbService.model.WebformTable" />
	<import class="com.rcs.dbService.model.WebformValue" />
	<class name="com.rcs.dbService.model.impl.WebformColumnImpl" table="RcsWebform_WebformColumn">
		<cache usage="read-write" />
		<id name="webformColumnId" type="long">
			<generator class="assigned" />
		</id>
		<property name="webformTableId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="type" type="com.liferay.portal.dao.orm.hibernate.IntegerType" column="type_" />
		<property name="inputType" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
		<property name="isRequired" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="placeHolder" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="validation" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="customValidation" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="labelClass" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="inputClass" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="maxLength" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.rcs.dbService.model.impl.WebformRowImpl" table="RcsWebform_WebformRow">
		<cache usage="read-write" />
		<id name="webformRowId" type="long">
			<generator class="assigned" />
		</id>
		<property name="webformTableId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="updateDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.rcs.dbService.model.impl.WebformTableImpl" table="RcsWebform_WebformTable">
		<cache usage="read-write" />
		<id name="webformTableId" type="long">
			<generator class="assigned" />
		</id>
		<property name="portletId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="webformCounter" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="webformName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.rcs.dbService.model.impl.WebformValueImpl" table="RcsWebform_WebformValue">
		<cache usage="read-write" />
		<id name="webformValueId" type="long">
			<generator class="assigned" />
		</id>
		<property name="webformTableId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="webformColumnId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="webformRowId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="data" type="com.liferay.portal.dao.orm.hibernate.StringType" column="data_" />
	</class>
</hibernate-mapping>